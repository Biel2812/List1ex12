 double x1;
    double x2;
    double x3;
    double x4;
    double x5;
    double vreferente;
    double troco;

    Console.WriteLine("Primeiro produto");
    x1 = double.Parse(Console.ReadLine());
    Console.WriteLine("Segundo produto");
    x2 = double.Parse(Console.ReadLine());
    Console.WriteLine("Terceiro produto");
    x3 = double.Parse(Console.ReadLine());
    Console.WriteLine("Quarto produto");
    x4 = double.Parse(Console.ReadLine());
    Console.WriteLine("Quinto produto");
    x5 = double.Parse(Console.ReadLine());
    Console.WriteLine("Seu valor referente");
    vreferente = double.Parse(Console.ReadLine());
    troco = vreferente-(x1+x2+x3+x4+x5);
    Console.WriteLine("Troco devolvido");
    Console.WriteLine(troco);
